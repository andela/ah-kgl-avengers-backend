{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./index.js --exec babel-node",
    "test": "nyc --reporter=text mocha --exit --require @babel/register --require @babel/polyfill ./tests/*",
    "dev": "nodemon index.js --exec babel-node",
    "build": "babel ./server --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start:heroku": "babel-node index.js",
    "heroku-postbuild": "npm install",
    "migration:generate": "node_modules/.bin/sequelize migrations:generate --name",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:refresh": "npm run migrate:reset && npm run migrate",
    "migrate:reset": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mocha": "^6.0.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0",
    "request": "^2.87.0",
    "sequelize": "^5.2.11",
    "slug": "^1.1.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "sequelize-cli": "^5.4.0",
    "underscore": "^1.9.1"
  }
}
